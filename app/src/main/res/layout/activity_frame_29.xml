                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_27pxv"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginStart="@dimen/_41pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_width="@dimen/_38pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_vector_55"/>
                    <ImageView android:id="@+id/image4"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_29pxv"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_46pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_25pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_vector_56"/>
                    <ImageView android:id="@+id/image5"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_29pxv"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginEnd="@dimen/_59pxh"
