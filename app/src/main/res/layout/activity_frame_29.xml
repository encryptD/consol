                        android:gravity="center" android:id="@+id/line1"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_2pxv"
                        android:layout_marginTop="@dimen/_3pxv" android:layout_width="@dimen/_18pxh"/>
