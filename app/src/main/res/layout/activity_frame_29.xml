                    android:id="@+id/viewPager"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="match_parent"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_weight="1" android:layout_width="match_parent"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
