                    android:id="@+id/tabLayout"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="match_parent"
                    app:tabIndicatorColor="@color/deep_purple_500"
                    app:tabIndicatorHeight="@dimen/_1pxv"
                    app:tabMode="fixed"
                    app:tabSelectedTextColor="@color/deep_purple_500" app:tabTextColor="@color/bluegray_900"/>
                <View android:background="@color/black_900"
                    android:id="@+id/line3"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_35pxh"
                    android:layout_marginStart="@dimen/_34pxh" android:layout_width="@dimen/_306pxh"/>
